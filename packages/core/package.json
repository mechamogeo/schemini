{
  "name": "@schemini/core",
  "version": "1.0.0",
  "description": "TypeScript-first schema validation library with JSON Schema support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./json-schema": {
      "types": "./dist/json-schema/index.d.ts",
      "import": "./dist/json-schema/index.js"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "schema",
    "validation",
    "typescript",
    "json-schema",
    "type-inference",
    "runtime-validation",
    "zod",
    "validator",
    "parse",
    "type-safe",
    "brazilian",
    "cpf",
    "cnpj",
    "schemini"
  ],
  "author": {
    "name": "Geovani Perez FranÃ§a",
    "email": "hi@geovani.dev",
    "url": "https://github.com/mechamogeo"
  },
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/mechamogeo"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mechamogeo/schemini.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/mechamogeo/schemini/issues"
  },
  "homepage": "https://github.com/mechamogeo/schemini#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.33"
  }
}
